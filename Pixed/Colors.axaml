<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Design.PreviewWith>
    <StackPanel>
      <Border Background="{StaticResource BaseBackground}" BorderBrush="{StaticResource BaseBorder}" BorderThickness="2" Width="128" Height="128"/>
      <Border Background="{StaticResource ButtonBackground}" BorderBrush="{StaticResource ButtonBorder}" BorderThickness="2" Width="128" Height="128"/>
      <Border Background="{StaticResource ButtonBackground}" BorderBrush="{StaticResource Accent}" BorderThickness="2" Width="128" Height="128"/>
    </StackPanel>
  </Design.PreviewWith>
  <Color x:Key="ButtonBorder">#888888</Color>
  <Color x:Key="BaseBorder">#444444</Color>
  <Color x:Key="BaseBackground">#1D1D1D</Color>
  <Color x:Key="Accent">#039BE5</Color>
  <Color x:Key="ButtonBackground">#3f3f3f</Color>
</ResourceDictionary>

<local:PixedWindow xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	         xmlns:local="clr-namespace:Pixed.Application.Windows;assembly=Pixed.Application"
             mc:Ignorable="d"
      d:DesignWidth="800"
      d:DesignHeight="450"
             VerticalAlignment="Center"
             HorizontalAlignment="Center"
             x:Class="Pixed.Application.Windows.SimpleDialog"
				   Title="Tu też działa">
	<local:PixedWindow.Dialog>
		<Canvas Background="#10000000" Width="200" Height="300">
			<TextBlock Foreground="Black" TextWrapping="Wrap" Width="400" VerticalAlignment="Stretch" Margin="10">
				Działa
			</TextBlock>
		</Canvas>
	</local:PixedWindow.Dialog>
</local:PixedWindow>

